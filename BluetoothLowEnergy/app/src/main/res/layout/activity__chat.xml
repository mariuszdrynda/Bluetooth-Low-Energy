<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="pz.bluetoothlowenergy.Activity_Chat">

    <Button
        android:id="@+id/wsteczLobby"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="Wstecz (Lobby)"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="8dp" />

    <Button
        android:id="@+id/zapiszLobby"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:onClick="initZapisz"
        android:text="Zapisz"
        app:layout_constraintTop_toBottomOf="@+id/wsteczLobby"
        tools:layout_editor_absoluteX="8dp" />
</android.support.constraint.ConstraintLayout>
